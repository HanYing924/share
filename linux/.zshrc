# Parameters: src_dir, src_file, dest_dir[, dest_file]
function symbolic_link() {
  if [ $# -lt 3 ]; then
    return 1
  fi

  if [ $# -eq 4 ]; then
    dest_file=$4
  else
    dest_file=$2
  fi

  if [ -d $3 -a ! -L $3/$dest_file ]; then
    ln -s $1/$2 $3/$dest_file
  fi
}

